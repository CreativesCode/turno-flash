{
  "name": "turno-flash",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && node scripts/ensure-platforms.js",
    "build:next": "next build",
    "start": "next start",
    "lint": "next lint",
    "cap:ensure:android": "node scripts/ensure-platform.js android",
    "cap:ensure:ios": "node scripts/ensure-platform.js ios",
    "cap:sync": "npm run build && npx cap sync",
    "cap:sync:android": "npm run cap:ensure:android && npm run build:next && npx cap sync android",
    "cap:sync:ios": "npm run cap:ensure:ios && npm run build:next && npx cap sync ios",
    "cap:ios": "npx cap open ios",
    "cap:android": "npx cap open android",
    "mobile:dev": "npm run build:next && npx cap sync && npm run dev",
    "mobile:build": "npm run build:next && npx cap sync",
    "mobile:build:ios": "npm run cap:sync:ios && npm run cap:ios",
    "mobile:build:android": "npm run cap:sync:android && npm run cap:android"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/cli": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/ios": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "@hookform/resolvers": "^5.2.2",
    "@supabase/supabase-js": "^2.90.1",
    "@tanstack/react-query": "^5.90.16",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.0",
    "sonner": "^2.0.7",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@supabase/ssr": "^0.8.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "supabase": "^2.72.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
